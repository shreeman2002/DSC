#include <stdio.h>

int main() {
    int a[5], key, low = 0, high = 4, mid, found = 0;

    printf("Enter 5 sorted elements:\n");
    for(int i = 0; i < 5; i++) {
        scanf("%d", &a[i]);
    }

    printf("Enter element to search: ");
    scanf("%d", &key);

    while(low <= high) {
        mid = (low + high) / 2;

        if(a[mid] == key) {
            printf("Element found at position %d", mid + 1);
            found = 1;
            break;
        }
        else if(a[mid] < key)
            low = mid + 1;
        else
            high = mid - 1;
    }

    if(!found)
        printf("Element not found");

    return 0;
}
